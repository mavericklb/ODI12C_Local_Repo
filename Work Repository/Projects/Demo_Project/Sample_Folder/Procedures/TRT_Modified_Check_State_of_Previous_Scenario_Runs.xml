 <Field name="Txt" type="java.lang.String"><![CDATA[/*
<$ 

import java.lang.Exception;
import java.sql.Connection;
import java.sql.Statement;
import java.sql.PreparedStatement;
import java.sql.ResultSet;

//Repository connection 
Connection vtmTgtconnForSrcUpg=odiRef.getJDBCConnection( "DEST" );
String vSrcPLVCurrent="<%=odiRef.getOption("OBI_PLV_CODE")%>".trim().toUpperCase();
//If Connection is null then raise Exception
if (vtmTgtconnForSrcUpg==null) {
	String vtmErrMesg="Custom Exception: Exception occured when trying to establish Repository Connection"+vtmNewline;
	TableException vtmSqlErr = new TableException(vtmErrMesg);			
	throw vtmSqlErr;
	}

//Query to get list of Load Plans other than the current that are still in Running/Waiting State
String  vtmSqlGetSourceUpgradeinfo;
vtmSqlGetSourceUpgradeinfo="Select 'Y' from dual where exists (SELECT 1 FROM W_ETL_SRC_VERSION_HISTORY WHERE  SRC_PLV_NEW IS NOT NULL AND SRC_UPGRADE_STATUS IN ('PENDING','ERROR','STARTED'))";
PreparedStatement vtmrStmtGetSourceUpgradeinfo=vtmTgtconnForSrcUpg.prepareStatement(vtmSqlGetSourceUpgradeinfo);

//Run the query
ResultSet vtmPrepRsGetSourceUpgradeinfo;
try{
	vtmPrepRsGetSourceUpgradeinfo=vtmrStmtGetSourceUpgradeinfo.executeQuery();
} catch (Exception e) {
	String vtmErrMesg="Custom Exception: Exception occured when trying to check if Load Plans are running:"+e.getMessage()+e.getCause();
	Exception vtmSqlErr = new Exception(vtmErrMesg);			
	throw vtmSqlErr;		
} //end catch

//Fetch the result
String SourceUpgradeFlag="N";
if (vtmPrepRsGetSourceUpgradeinfo.next()) {
	SourceUpgradeFlag=vtmPrepRsGetSourceUpgradeinfo.getString(1);
} 

if(SourceUpgradeFlag.equals("Y")) {
    String lpErr=">>>>>Found Incomplete Source Upgrade status from the Source Upgrade tracking table (W_ETL_SRC_VERSION_HISTORY).For more information, please refer to the Warehouse table W_ETL_SRC_VERSION_HISTORY for incomplete Source Upgrade status rows and take necessary action to run Source Upgrade Load plans <<<<<<";
	out.println(lpErr);
	Exception vtmLPErr = new Exception(lpErr);			
	throw vtmLPErr;
	
} else {
	out.println("--No Incomplete Source Upgrade status found from W_ETL_SRC_VERSION_HISTORY table ");
}

vtmTgtconnForSrcUpg.close();

$>

*/
]]></Field>
