//Repository connection 
Connection vtmTgtconn=odiRef.getJDBCConnection( "DEST" );
//If Connection is null then raise Exception
if (vtmTgtconn==null) {
	String vtmErrMesg="Custom Exception: Exception occured when trying to establish Repository Connection"+vtmNewline;
	TableException vtmSqlErr = new TableException(vtmErrMesg);			
	throw vtmSqlErr;
	}

//Query to get list of Load Plans other than the current that are still in Running/Waiting State
String  vtmSqlSelGetBlackoutSch;
vtmSqlSelGetBlackoutSch="Select 'Y' from dual where exists (select -1 from w_etl_control_schedule cs where sysdate between cs.control_start_dt and cs.control_end_dt and cs.control_code = 'NO_ETL')";
PreparedStatement vtmrStmtGetBlackoutSch=vtmTgtconn.prepareStatement(vtmSqlSelGetBlackoutSch);

//Run the query
ResultSet vtmPrepRsLpBlackoutSch;
try{
	vtmPrepRsLpBlackoutSch=vtmrStmtGetBlackoutSch.executeQuery();
} catch (Exception e) {
	String vtmErrMesg="Custom Exception: Exception occured when trying to check if Load Plans are running:"+e.getMessage()+e.getCause();
	Exception vtmSqlErr = new Exception(vtmErrMesg);			
	throw vtmSqlErr;		
} //end catch

//Fetch the result
String ctLPBlackoutSch="N";
if (vtmPrepRsLpBlackoutSch.next()) {
	ctLPBlackoutSch=vtmPrepRsLpBlackoutSch.getString(1);
} 

if(ctLPBlackoutSch.equals("Y")) {
    String lpErr=">>>>> Blackout schedule coming up as seen in w_etl_control_schedule table, hence no Load plan(s) will be executed. ABORTING this Load Plan as there will be a down time to the repository due to the Blackout schedule <<<<<<";
	out.println(lpErr);
	Exception vtmLPErr = new Exception(lpErr);			
	throw vtmLPErr;
	
} else {
	out.println("--No Blackout schedule period found from w_etl_control_schedule table ");
}

vtmTgtconn.close();

$>

