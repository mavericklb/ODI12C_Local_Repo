import java.lang.Exception;
import java.sql.Connection;
import java.sql.Statement;
import java.sql.PreparedStatement;
import java.sql.ResultSet;

//Repository connection 
Connection vtmRepconn=odiRef.getJDBCConnection( "WORKREP" );
//If Connection is null then raise Exception
if (vtmRepconn==null) {
	String vtmErrMesg="Custom Exception: Exception occured when trying to establish Repository Connection"+vtmNewline;
	TableException vtmSqlErr = new TableException(vtmErrMesg);			
	throw vtmSqlErr;
	}

//Query to get list of Load Plans other than the current that are still in Running/Waiting State
String  vtmSqlSelGetIsLPRunning;
vtmSqlSelGetIsLPRunning=" SELECT COUNT(1) FROM  SNP_LPI_RUN, SNP_LP_INST WHERE SNP_LP_INST.I_LP_INST=SNP_LPI_RUN.I_LP_INST AND SNP_LPI_RUN.STATUS IN ('R','W') AND SNP_LPI_RUN.I_LP_INST!=(?) AND SNP_LP_INST.LOAD_PLAN_NAME='<%=odiRef.getLoadPlanInstance("LOAD_PLAN_NAME")%>' ";
PreparedStatement vtmrStmtGetIsRunning=vtmRepconn.prepareStatement(vtmSqlSelGetIsLPRunning);

vtmrStmtGetIsRunning.setInt(1,Integer.parseInt("<%=odiRef.getLoadPlanInstance("BATCH_ID")%>")); //Bind the variables


//Run the query
ResultSet vtmPrepRsLpRun;
try{
	vtmPrepRsLpRun=vtmrStmtGetIsRunning.executeQuery();
} catch (Exception e) {
	String vtmErrMesg="Custom Exception: Exception occured when trying to check if Load Plans are running:"+e.getMessage()+e.getCause();
	Exception vtmSqlErr = new Exception(vtmErrMesg);			
	throw vtmSqlErr;		
} //end catch

//Fetch the result
int ctLPRunning=0;
if (vtmPrepRsLpRun.next()) {
	ctLPRunning=vtmPrepRsLpRun.getInt(1);
} 

if(ctLPRunning==0)
	out.println("--No Load plan found to be running/waiting apart from the current one");
else {
	String lpErr=">>>>>" + ctLPRunning + " Load plan(s) found to be running/waiting apart from the current one. ABORTING this Load Plan as running multiple Load Plans in Parallel can cause DATA CORRUPTION <<<<<<";
	out.println(lpErr);
	Exception vtmLPErr = new Exception(lpErr);			
	throw vtmLPErr;
}

$>

*/
